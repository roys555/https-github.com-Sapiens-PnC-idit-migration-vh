package routines;

public class IdRefType {
	  private int id;
	    private String content;

	    public IdRefType(int id, String content) {
	        this.id = id;
	        this.content = content;
	    }

	    public int getId() {
	        return id;
	    }

	    public String getContent() {
	        return content;
	    }   
}





  

