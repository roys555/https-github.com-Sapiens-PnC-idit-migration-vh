public class ContactProcessingAGR {

	/// AddressIVO -> CountryVORef
    public static GenericVORef returnCountryVORef(String i_code) {
    	i_code =" D  ";
    	i_code = i_code.trim();
    	if(i_code.equals( "A"))       {	return new GenericVORef(13, "AUSTRIA"); }
    	else if (i_code == "B") {	return new GenericVORef(1, "BELGIUM"); }
    	else if (i_code == "CH") {	return new GenericVORef(195, "SWITZERLAND"); }
    	else if (i_code == "CZ") {	return new GenericVORef(54, "CZECH REPUBLIC"); }
    	else if (i_code.equals ("D")) {	return new GenericVORef(77, "GERMANY"); }
    	else if (i_code == "DK") {	return new GenericVORef(55, "DENMARK"); }
    	else if (i_code == "E") {	return new GenericVORef(64, "ESTONIA"); }
    	else {    	
    		return new GenericVORef(-1, i_code);
    	}
    }
    
   /// ContactBankAccountIVO -> bankAccountExternalNumber logic   
    
    public static String returnBankAccountExternalNumber(long partnerID, long bankAccountId, boolean isTestLoad, int migrationID) {
    	String external = "MIG-" + Long.toString(partnerID) + "-" + Long.toString(bankAccountId);
    	if(isTestLoad) {
    		external = external +"_"+Integer.toString(migrationID);
    	} 	
    	return external;
    }

    
   /// ContactBankAccountIVO -> countryVORef

    public static GenericVORef returnCountryVORefFromIBAN(String iban) {    	
    	iban = iban.trim();
    	if(iban.startsWith("A")){			return new GenericVORef(13, "AUSTRIA"); }
    	else if (iban.startsWith("B")) {	return new GenericVORef(1, "BELGIUM"); }
    	else if (iban.startsWith("CH")) {	return new GenericVORef(195, "SWITZERLAND"); }
    	else if (iban.startsWith("CZ")) {	return new GenericVORef(54, "CZECH REPUBLIC"); }
    	else if (iban.startsWith("DK")) {	return new GenericVORef(55, "DENMARK"); }
    	else if (iban.startsWith("D")) {	return new GenericVORef(77, "GERMANY"); }
    	else if (iban.startsWith("E")) {	return new GenericVORef(64, "ESTONIA"); }
    	else if (iban.startsWith("F")) {	return new GenericVORef(70, "FRANCE"); }
    	else if (iban.startsWith("HR")) {	return new GenericVORef(51, "CROATIA"); }
    	else if (iban.startsWith("I")) {	return new GenericVORef(100, "ITALY"); }    	
    	else if (iban.startsWith("LT")) {	return new GenericVORef(118, "LITHUANIA"); }
    	else if (iban.startsWith("LV")) {	return new GenericVORef(112, "LATVIA"); }
    	else if (iban.startsWith("L")) {	return new GenericVORef(119, "LUXEMBOURG"); }
    	else if (iban.startsWith("MOL")) {	return new GenericVORef(135, "MOLDOVA"); }
    	else if (iban.startsWith("NL")) {	return new GenericVORef(145, "NETHERLANDS"); }
    	else if (iban.startsWith("PL")) {	return new GenericVORef(163, "POLAND"); }
    	else if (iban.startsWith("RO")) {	return new GenericVORef(168, "ROMANIA"); }
    	else if (iban.startsWith("S")) {	return new GenericVORef(186, "SPAIN"); }
    	else {    	
    		return new GenericVORef(-1, iban);
    	}
    }
}
